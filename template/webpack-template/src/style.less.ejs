q::after {
	content: '';
	content: none;
}

table {
	border-spacing: 0;
	border-collapse: collapse;
}

a:focus {
	outline: thin dotted;
}

a:active,
a:hover {
	outline: 0;
}

html {
	-webkit-tap-highlight-color: transparent;
	-webkit-font-smoothing: antialiased;
}

